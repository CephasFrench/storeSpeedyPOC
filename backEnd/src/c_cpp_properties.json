{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/homebrew/opt/sdl2/include/SDL2",
                "/opt/homebrew/opt/sdl2_gfx/include/SDL2",
                "/opt/homebrew/opt/opencv/include/opencv4",
                "/opt/X11/include",
                "/opt/homebrew/opt/jsoncpp/include",
                "/opt/homebrew/opt/poppler/include"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-L/opt/homebrew/opt/sdl2/lib",
                "-L/opt/homebrew/opt/sdl2_gfx/lib",
                "-L/opt/homebrew/opt/opencv/lib",
                "-L/opt/X11/lib",
                "-L/opt/homebrew/opt/jsoncpp/lib",
                "-L/opt/homebrew/opt/poppler/lib",
                "-lSDL2",
                "-lSDL2_gfx",
                "-lopencv_core",
                "-lopencv_imgcodecs",
                "-lopencv_imgproc",
                "-lopencv_highgui",
                "-lX11",
                "-ljsoncpp",
                "-std=c++11"
            ]
        }
    ],
    "version": 4
}
